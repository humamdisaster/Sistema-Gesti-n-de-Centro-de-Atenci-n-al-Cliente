        <language>java</language>
        <body>if (!inComboBox(&quot;Elija un cliente&quot;)) {&#xD;
	        comboBox.insertItemAt(&quot;Elija un cliente&quot;, 0);&#xD;
	    }&#xD;
		comboBox.setSelectedItem(&quot;Elija un cliente&quot;); // opción por defecto&#xD;
     textName.setText(&quot;&quot;);&#xD;
     textMail.setText(&quot;&quot;);&#xD;
     textDesc.setText(&quot;&quot;);&#xD;
 </body>

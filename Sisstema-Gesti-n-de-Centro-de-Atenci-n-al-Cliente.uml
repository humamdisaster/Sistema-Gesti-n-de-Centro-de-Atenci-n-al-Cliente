      <ownedAttribute xmi:id="_8YwmkJvnEfCpAP_kYF-EZg" name="panelPrincipal" visibility="private" type="_8L-u0JvnEfCpAP_kYF-EZg" association="_8qWF0ZvnEfCpAP_kYF-EZg"/>
      <ownedAttribute xmi:id="_8Yx0sJvnEfCpAP_kYF-EZg" name="areaTickets" visibility="private" type="_8M5U0JvnEfCpAP_kYF-EZg" association="_8qYiEZvnEfCpAP_kYF-EZg"/>

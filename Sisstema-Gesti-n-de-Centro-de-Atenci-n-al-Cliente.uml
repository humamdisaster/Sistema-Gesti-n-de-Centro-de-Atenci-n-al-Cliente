      <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_8jvHgJvnEfCpAP_kYF-EZg" name="generarReporte" specification="_7KEl4JvnEfCpAP_kYF-EZg">
        <ownedParameter xmi:id="_8jvHgZvnEfCpAP_kYF-EZg" name="nombreArchivo">

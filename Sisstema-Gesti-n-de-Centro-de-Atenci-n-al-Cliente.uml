      <ownedAttribute xmi:id="_8YZaM5vnEfCpAP_kYF-EZg" name="btnVolver" visibility="private" type="_8M6i8ZvnEfCpAP_kYF-EZg" association="_8qIDYJvnEfCpAP_kYF-EZg"/>

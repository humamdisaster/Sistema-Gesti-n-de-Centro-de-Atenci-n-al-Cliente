        <language>java</language>
        <body>this.listener = listener;&#xD;
 </body>
      </ownedBehavior>
      <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_8kA0UpvnEfCpAP_kYF-EZg" name="mostrarTickets" specification="_8RuxgJvnEfCpAP_kYF-EZg">
        <ownedParameter xmi:id="_8kA0U5vnEfCpAP_kYF-EZg" name="clientes" type="_8NM20JvnEfCpAP_kYF-EZg"/>
        <language>java</language>
        <body>areaTickets.setText(&quot;&quot;);&#xD;
     for (Cliente cliente : clientes.values()) {&#xD;
         areaTickets.append(&quot;Tickets de &quot; + cliente.info(true) + &quot;\n&quot;);&#xD;
         for (Ticket ticket : cliente.getTickets()) {&#xD;
             areaTickets.append(&quot; - &quot; + ticket.resumen(true) + &quot;\n&quot;);&#xD;
         }&#xD;
         areaTickets.append(&quot;\n&quot;);&#xD;
     }&#xD;
 </body>
      </ownedBehavior>
